<% #SmallAlert %>
 <!-- Alert -->
            <div id="cen-alert-small" class=" w3-card-4 cen-round w3-mobile w3-animate-opacity" style="max-width:400px;min-width:300px;position:fixed;top:100px;left:50%;transform:translate(-50%,0%);-ms-transform:translate(-50%,0%);z-index:1000;display:none;">
                <img id="cen-alert-logo" class="cen-alert-top-logo" src="aim://me://images/resourse/info.png" />
